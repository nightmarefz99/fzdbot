from random import choice

def get_response(user_input: str) -> str:
    #raise NotImplementedError('TBD...')
    input_lowered = user_input.lower()  

    if input_lowered == '':
        return "Don't be shy, say something!"
    elif 'hello' in input_lowered:
        return "Hello World!!!"
    else:
        return choice(['Sorry i do not understand...',
		       'I am not an AI...',
		       'What do you mean?'])
 
